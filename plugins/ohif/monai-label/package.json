  "license": "MIT",
  "repository": "OHIF/Viewers",
  "main": "dist/monailabel.umd.js",
  "module": "src/index.js",
  "sideEffects": true,
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=10",
    "npm": ">=6",
    "yarn": ">=1.16.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --config .webpack/webpack.dev.js --watch --debug --output-pathinfo",
