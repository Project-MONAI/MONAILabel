    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "dependencies": {
    "@babel/runtime": "^7.5.5",
    "axios": "^0.21.1",
    "arraybuffer-concat": "^0.0.1",
    "ndarray": "^1.0.19",
    "md5.js": "^1.3.5",
    "nrrd-js": "^0.2.1",
    "pako": "^2.0.3",
    "itk": "^14.1.1",
    "react-color": "^2.19.3",
